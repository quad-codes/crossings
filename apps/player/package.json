{
  "name": "player",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "doctor": "pnpx expo-doctor@latest",
    "doctor:fix": "expo install --check",
    "bump": "./scripts/bump-build",
    "build:dev": "pnpm run bump && pnpm run eas build --platform all --profile development --non-interactive",
    "build:devdevice": "pnpm run eas build --platform all --profile development-device --non-interactive",
    "build:prod": "pnpm run eas build --platform all --non-interactive",
    "devclient:ios": "pnpm run eas build:run -p ios",
    "devclient:android": "pnpm run eas build:run -p android",
    "submit": "pnpm run eas submit --platform all --non-interactive",
    "eas": "pnpx eas-cli@latest"
  },
  "dependencies": {
    "@shopify/flash-list": "1.6.4",
    "@tanstack/react-query": "^5.40.1",
    "expo": "^51.0.11",
    "expo-dev-client": "~4.0.15",
    "expo-router": "~3.5.15",
    "jotai": "^2.8.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "twrnc": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@types/lodash": "^4.17.4",
    "@types/luxon": "^3.4.2",
    "@types/react": "~18.2.79",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
