{
	"cli": {
		"version": ">= 12.0.0",
		"promptToConfigurePushNotifications": false,
		"appVersionSource": "remote"
	},
	"build": {
		"production": {
			"pnpm": "9.0.6",
			"ios": { "image": "latest" },
			"android": { "image": "latest" },
			"autoIncrement": true,
			"channel": "production"
		},
		"development": {
			"extends": "production",
			"developmentClient": true,
			"env": {
				"APP_VARIANT": "development"
			},
			"distribution": "internal",
			"ios": { "simulator": true },
			"channel": "development"
		},
		"development-device": {
			"extends": "production",
			"developmentClient": true,
			"env": {
				"APP_VARIANT": "development-device"
			},
			"distribution": "internal",
			"channel": "development"
		}
	},
	"submit": {
		"production": {
			"ios": {
				"appleId": "pvinis@gmail.com",
				"appleTeamId": "CAG2W9M777",
				"ascAppId": "6477339034"
			},
			"android": {
				"track": "internal",
				"releaseStatus": "draft"
			}
		}
	}
}
