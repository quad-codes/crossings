{
  "name": "player",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "APP_VARIANT=development expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "doctor": "pnpx expo-doctor@latest",
    "doctor:fix": "expo install --check",
    "bump": "pnpx node-jq '.build = .build+1' version.json | pnpx sponge version.json; pnpx prettier -w version.json",
    "build:dev": "pnpm run eas build --platform all --profile development",
    "build:devdevice": "pnpm run eas build --platform all --profile development-device --non-interactive",
    "build:prod": "pnpm run eas build --platform all --non-interactive",
    "bbuild:dev": "pnpm run bump ; pnpm run build:dev",
    "bbuild:devdevice": "pnpm run bump ; pnpm run build:devdevice",
    "bbuild:prod": "pnpm run bump ; pnpm run build:prod",
    "devclient": "pnpm run devclient:ios ; pnpm run devclient:android",
    "devclient:ios": "pnpm run eas build:run -p ios --latest",
    "devclient:android": "pnpm run eas build:run -p android --latest",
    "submit": "pnpm run eas submit --platform all",
    "db:init": "pnpx supabase login ; pnpx supabase init ; pnpx supabase link",
    "db:gen-types": "pnpx supabase gen types typescript --linked --schema=public > src/generated/database.types.ts",
    "test:ui": "maestro test trial.yaml",
    "test:ui:studio": "maestro studio",
    "eas": "pnpx eas-cli@latest"
  },
  "dependencies": {
    "@shopify/flash-list": "1.6.4",
    "@supabase-cache-helpers/postgrest-react-query": "^1.7.0",
    "@supabase/supabase-js": "^2.44.2",
    "@tanstack/react-query": "^5.50.1",
    "expo": "^51.0.18",
    "expo-application": "~5.9.1",
    "expo-dev-client": "~4.0.19",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.7",
    "expo-insights": "~0.7.0",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-notifications": "~0.28.9",
    "expo-router": "~3.5.17",
    "expo-updates": "~0.25.18",
    "humanize-duration": "^3.32.1",
    "jotai": "^2.8.4",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "posthog-react-native": "^3.1.1",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-calendars": "^1.1305.0",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-share": "^10.2.1",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-use": "^17.5.0",
    "twrnc": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@types/humanize-duration": "^3.27.4",
    "@types/lodash": "^4.17.6",
    "@types/luxon": "^3.4.2",
    "@types/react": "~18.2.79",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react-native-svg-transformer": "^1.4.0",
    "tailwindcss": "^3.4.4",
    "tailwindcss-safe-area": "^0.5.1",
    "typescript": "~5.3.3"
  }
}
