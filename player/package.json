{
  "name": "player",
  "version": "0.7.1",
  "main": "expo-router/entry",
  "scripts": {
    "set-lang": "run(){ ./scripts/set-lang.js $1 ; pnpx prettier -w extraConfig.json ;} ; run",
    "start": "APP_VARIANT=development expo start",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "doctor": "pnpx expo-doctor@latest",
    "doctor:fix": "expo install --check",
    "build:dev": "pnpm run eas build --platform all --profile development --non-interactive",
    "build:devdevice": "pnpm run eas build --platform all --profile development-device --non-interactive",
    "build:prod": "pnpm run eas build --platform all --non-interactive --auto-submit",
    "devclient": "pnpm run devclient:ios ; pnpm run devclient:android",
    "devclient:ios": "pnpm run eas build:run -p ios --latest",
    "devclient:android": "pnpm run eas build:run -p android --latest",
    "submit": "pnpm run eas submit --platform all",
    "updatejs": "pnpm eas update --branch production --message",
    "db:init": "pnpx supabase login ; pnpx supabase init ; pnpx supabase link",
    "db:gen-types": "pnpx supabase gen types typescript --linked --schema=public > src/generated/database.types.ts",
    "test:ui": "maestro test trial.yaml",
    "test:ui:studio": "maestro studio",
    "eas": "pnpx eas-cli@latest"
  },
  "dependencies": {
    "@sentry/react-native": "~5.31.1",
    "@shopify/flash-list": "1.6.4",
    "@supabase-cache-helpers/postgrest-react-query": "^1.10.1",
    "@supabase/supabase-js": "2.45.3",
    "@tanstack/react-query": "5.55.0",
    "clsx": "2.1.1",
    "expo": "~51.0.32",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.26",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-insights": "~0.7.0",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-notifications": "~0.28.16",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "^0.27.5",
    "expo-updates": "~0.25.24",
    "humanize-duration": "3.32.1",
    "jotai": "2.9.3",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "nativewind": "4.1.6",
    "posthog-react-native": "3.1.2",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-calendars": "1.1306.0",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-mmkv": "2.12.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-share": "11.0.2",
    "react-native-svg": "15.2.0",
    "react-native-url-polyfill": "2.0.0",
    "react-use": "17.5.1",
    "semver": "7.6.3",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.10"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@changesets/cli": "^2.27.8",
    "@types/humanize-duration": "3.27.4",
    "@types/lodash": "4.17.7",
    "@types/luxon": "3.4.2",
    "@types/react": "~18.2.79",
    "node-jq": "6.0.1",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.6",
    "react-native-svg-transformer": "^1.5.0",
    "typescript": "~5.3.3"
  }
}
